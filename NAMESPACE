# Export all names
exportPattern(".")

# This is what R CMD check --as-cran suggests:

importFrom("grDevices", "adjustcolor", "boxplot.stats", "chull",
           "col2rgb", "colors", "dev.off", "grey", "palette", "pdf")

importFrom("graphics", "abline", "arrows", "axis", "barplot", "box",
